{
  "diagnostic.enable": false,
  "coc.preferences.hoverTarget": "preview",
  "languageserver": {
    "golang": {
      "command": "gopls",
      "args": ["-mode","stdio"],
      "rootPatterns": ["go.mod", ".vim/", ".git/", ".hg/"],
      "filetypes": ["go"]
    },
    "dockerfile": {
      "command": "docker-langserver",
      "filetypes": ["dockerfile"],
      "args": ["--stdio"]
    }
  }
}
